  "license": "MIT",
  "dependencies": {
    "@prisma/client": "4.6.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pdfmake": "^0.2.6",
    "ts-results": "^3.3.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "env:dev": "dotenv -e .env.development --",
    "env:prod": "dotenv -e .env.prod --",
    "build": "yarn env:dev babel src -d dist --extensions '.ts' --watch",
    "dev": "yarn env:dev nodemon dist/main.js",
    "start": "yarn concurrently --kill-others 'yarn build' 'yarn dev'",
    "migrate:dev": "yarn env:dev yarn prisma migrate dev --create-only",
    "migrate:dev:reset": "yarn env:dev yarn prisma migrate reset --force",
    "migrate:prod": "yarn env:prod yarn prisma migrate dev",
    "migrate:prod:reset": "yarn env:prod yarn prisma migrate reset",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.8.1",
    "@types/passport": "^1.0.11",
    "@types/passport-local": "^1.0.34",
    "@types/pdfmake": "^0.2.2",
    "babel-jest": "^29.3.1",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.4.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
